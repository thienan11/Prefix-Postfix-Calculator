<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" maxHeight="650.0" maxWidth="650.0" minHeight="650.0" minWidth="650.0" prefHeight="650.0" prefWidth="650.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="calculator.project2calculatorgui.CalculatorController">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Enter expression here:" />
   <TextField fx:id="expressionText" alignment="CENTER_RIGHT">
      <font>
         <Font size="20.0" />
      </font>
   </TextField>
   <HBox alignment="CENTER" prefHeight="0.0" prefWidth="800.0" spacing="100.0">
      <children>
         <RadioButton fx:id="prefixRadioButton" mnemonicParsing="false" text="Prefix" />
         <RadioButton fx:id="postfixRadioButton" mnemonicParsing="false" text="Postfix" />
      </children>
   </HBox>
   <HBox alignment="CENTER" prefHeight="250.0" prefWidth="250.0">
      <children>
         <GridPane fx:id="gridPaneNum" alignment="CENTER">
           <columnConstraints>
             <ColumnConstraints hgrow="SOMETIMES" percentWidth="0.0" />
             <ColumnConstraints hgrow="SOMETIMES" percentWidth="0.0" />
               <ColumnConstraints percentWidth="0.0" />
               <ColumnConstraints percentWidth="0.0" />
               <ColumnConstraints />
           </columnConstraints>
           <rowConstraints>
             <RowConstraints percentHeight="0.0" vgrow="SOMETIMES" />
             <RowConstraints percentHeight="0.0" vgrow="SOMETIMES" />
             <RowConstraints percentHeight="0.0" vgrow="SOMETIMES" />
               <RowConstraints percentHeight="0.0" />
           </rowConstraints>
            <children>
         
                       <Button prefHeight="50.0" prefWidth="50.0" text="7" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                 <Button prefHeight="50.0" prefWidth="50.0" text="8" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                 <Button prefHeight="50.0" prefWidth="50.0" text="9" GridPane.columnIndex="2" GridPane.rowIndex="0" />
                 <Button prefHeight="50.0" prefWidth="50.0" text="4" GridPane.rowIndex="1" />
                 <Button prefHeight="50.0" prefWidth="50.0" text="5" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                 <Button prefHeight="50.0" prefWidth="50.0" text="6" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                 <Button prefHeight="50.0" prefWidth="50.0" text="1" GridPane.columnIndex="0" GridPane.rowIndex="2" />
                 <Button prefHeight="50.0" prefWidth="50.0" text="2" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                 <Button prefHeight="50.0" prefWidth="50.0" text="3" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                 <Button prefHeight="50.0" prefWidth="50.0" text="0" GridPane.rowIndex="3" />
               <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="space" GridPane.columnIndex="2" GridPane.rowIndex="3" />
               <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="." GridPane.columnIndex="1" GridPane.rowIndex="3">
                  <font>
                     <Font size="20.0" />
                  </font>
               </Button>
            </children>
         </GridPane>
         <GridPane fx:id="gridPaneOp" alignment="CENTER" prefHeight="100.0" prefWidth="150.0">
           <columnConstraints>
             <ColumnConstraints hgrow="SOMETIMES" percentWidth="0.0" />
             <ColumnConstraints hgrow="SOMETIMES" percentWidth="0.0" />
               <ColumnConstraints hgrow="SOMETIMES" percentWidth="0.0" />
               <ColumnConstraints hgrow="SOMETIMES" percentWidth="0.0" />
               <ColumnConstraints hgrow="SOMETIMES" percentWidth="0.0" />
           </columnConstraints>
           <rowConstraints>
             <RowConstraints percentHeight="0.0" vgrow="SOMETIMES" />
             <RowConstraints percentHeight="0.0" vgrow="SOMETIMES" />
             <RowConstraints percentHeight="0.0" vgrow="SOMETIMES" />
               <RowConstraints percentHeight="0.0" vgrow="SOMETIMES" />
           </rowConstraints>
            <HBox.margin>
               <Insets />
            </HBox.margin>
            <children>
               <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="+" />
               <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="*" GridPane.rowIndex="1" />
               <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="^" GridPane.rowIndex="2" />
               <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="-" GridPane.columnIndex="1" />
               <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="/" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="\%" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="clear" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         
                                      <Button prefHeight="50.0" prefWidth="50.0" text="=" GridPane.columnSpan="4" GridPane.rowIndex="3" />
            </children>
         </GridPane>
      </children>
   </HBox>
   <VBox prefHeight="250.0" prefWidth="630.0">
      <children>
         <GridPane hgap="5.0">
           <columnConstraints>
             <ColumnConstraints hgrow="SOMETIMES" percentWidth="0.0" />
             <ColumnConstraints hgrow="SOMETIMES" percentWidth="0.0" />
               <ColumnConstraints hgrow="SOMETIMES" percentWidth="0.0" />
               <ColumnConstraints hgrow="SOMETIMES" percentWidth="0.0" />
           </columnConstraints>
           <rowConstraints>
               <RowConstraints percentHeight="0.0" vgrow="SOMETIMES" />
             <RowConstraints percentHeight="0.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints percentHeight="0.0" vgrow="SOMETIMES" />
           </rowConstraints>
            <children>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Variable Name" />
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Variable Value" GridPane.columnIndex="2" />
               <TextField fx:id="varNameText" onKeyPressed="#checkVarName" prefHeight="25.0" prefWidth="132.0" GridPane.rowIndex="1" />
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="=" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               <TextField fx:id="varValText" onKeyPressed="#checkVarVal" prefHeight="25.0" prefWidth="132.0" GridPane.columnIndex="2" GridPane.rowIndex="1" />
               <Button fx:id="varButton" mnemonicParsing="false" text="Add/Edit" GridPane.columnIndex="3" GridPane.rowIndex="1" />
            </children>
            <VBox.margin>
               <Insets />
            </VBox.margin>
         </GridPane>
         <HBox alignment="CENTER" prefHeight="270.0" prefWidth="800.0">
            <children>
               <ListView fx:id="nameList" prefHeight="200.0" prefWidth="200.0" />
               <ListView fx:id="valList" prefHeight="200.0" prefWidth="200.0" />
               <TableView fx:id="historyTableView" fixedCellSize="0.0" prefHeight="400.0" prefWidth="400.0">
                  <columnResizePolicy>
                     <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                  </columnResizePolicy>
                  <columns>
                     <TableColumn fx:id="historyTableColumn" editable="false" prefWidth="75.0" sortType="DESCENDING" sortable="false" text="History" />
                  </columns>
               </TableView>
            </children>
         </HBox>
      </children>
   </VBox>
</VBox>
